	color:white;
}


.bg-grey{
	background-color:#707070;
}

#customers tr:nth-child(even){background-color: #f2f2f2;}

#customers tr:hover {background-color: #ddd;}

#customers th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: left;
  background-color: #EFDA20;
  color: white;
}

#customers i {
    display: block;
    text-align: center;
    margin-bottom: 9px;
    font-size: 28px;
    color: #707070 !important;
    font-weight: bold;
}

#yellow tr:nth-child(even) {
    background-color: #f2f2f2;
}


#yellow {
  border-collapse: collapse;
