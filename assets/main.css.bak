@font-face {
    font-family: nexalight;
    src: url(./fonts/Nexa-ExtraLight.ttf);
}
  
@font-face {
    font-family: nexaheavy;
    src: url(./fonts/Nexa-Heavy.ttf);
}
  
@font-face {
    font-family: nexaregular;
    src: url(./fonts/Nexa\ Regular.otf);
}

.font-bold{
	font-weight:bold;
}



.btn-secondary {
    color: #fff;
    background-color: #707070;
    border-color: #707070;
    padding: 10px 26px;
    border-radius: 0;
}

.fa-circle:before {
    color: black;
}

.news-category{
	color:grey;
}

.news-title{
	font-weight:bold;
}

.blog-date{
	background-color: #E1A631;
    padding: 10px;
    border-radius: 13px;
    color: white;
	display: inline-flex;
}

.modal-header {
    padding: 0rem 0rem !important;
    background-color: #EFDA20;
    padding: 0px 0px 0px 9px !important;
	color:white;
}

.modal-header .close{
	    cursor: pointer;
    border: none;
    height: 47px;
    width: 44px;
    font-size: 34px;
}

.bg-grey th{
	background-color:grey;
}


body{
    font-family: nexaregular;
}

@media (min-width: 992px){
.container, .container-lg, .container-md, .container-sm, .container-xl, .container-xxl {
    max-width: 1180px !important;
}
.table-responsive {
    overflow-x: unset !important;
    -webkit-overflow-scrolling: touch;
}

}

@media (max-width:992px){
	
	.main-content {
    padding: 3rem 0rem 4rem !important;
}
.left {
    float: left;
    margin-top: 5px !important;
    position: absolute;
}
.w-45 {
    width: 97%!important;
}
.breadcrumb{
	height:100px;
}
#customers {
    font-size: 11px;
}
#customers td, #customers th {
    padding: 6px 6px !important;
}
.mt-7 {
    margin-top: 2rem !important;
}
.bg-h3 h3 {
    font-size: 3rem !important;
    opacity: 0.1;
}
.news-box .box {
    padding: 2rem 2rem !important;
}
.bg-h3 {
    top: 66px !important;
}
img{
	width:100%;
}

}

@media (max-width:1340px){
	.mobile-none{
		display:none !important;
	}
}


.alpemix-contact p{
	margin-bottom:0px;
}

.bg-h3{
	position: absolute;
    width: 100%;
    text-align: center;
    z-index: -99;
    top: 43px;
}

.mt-7{
	margin-top:4rem;
}

.bg-h3 h3{
	    font-size: 8rem;
    opacity: 0.1;
}

.btn-dark {
    color: #fff;
    background-color: #212529;
    border-color: #212529;
    padding: 9px 42px;
    border-radius: 0px;
    font-weight: bold;
    margin-top: 27px;
}

a{
	color:black;
	text-decoration:none
}

.news-box .box{
	box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;
    padding: 2rem 5rem;
    background-color: white;
	margin-top:25px;
}

